package groupdisscussion_day1;

public class pgm1 {

	public static void main(String[] args) {
		int k=5;
		for(int i=0;i<5;i++)
		{
			for(k=5;k>i;k--)
				{
					System.out.print(" ");
				}
				for(int j=0;j<=i;j++)
				{
					System.out.print("*");
				}
				k=k-1;
			System.out.println();
		}



	}

}
