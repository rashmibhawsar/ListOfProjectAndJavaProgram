package day13;

import java.util.Arrays;

public class AnagramString {

	public static void main(String[] args) {

		char arr1[]= {'a','b','c'};
		char arr2[]= {'b','a','c'};
		Arrays.sort(arr1);
		Arrays.sort(arr2);
		for(int i =0;i<3;i++) {
			if(arr1[i]==arr2[i])
				continue;
			else {System.out.println("its not anagram");
			break;
			}
		}

	}

}
